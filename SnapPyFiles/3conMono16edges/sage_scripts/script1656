import snappy
A=snappy.Manifold('/home/cris/SnapPy/1656_T320.tri')
B=snappy.Manifold('/home/cris/SnapPy/1656_T357.tri')
C=snappy.Manifold('/home/cris/SnapPy/1656_T405.tri')
D=snappy.Manifold('/home/cris/SnapPy/1656_T421.tri')
coversA=A.covers(5,method='gap')
coversB=B.covers(5,method='gap')
coversC=C.covers(5,method='gap')
coversD=D.covers(5,method='gap')
[X.homology() for X in coversA]
[X.homology() for X in coversB]
[X.homology() for X in coversC]
[X.homology() for X in coversD]
