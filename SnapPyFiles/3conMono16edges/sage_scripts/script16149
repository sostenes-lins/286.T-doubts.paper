import snappy
A=snappy.Manifold('/home/cris/SnapPy/16149_T437.tri')
B=snappy.Manifold('/home/cris/SnapPy/16149_T449.tri')
C=snappy.Manifold('/home/cris/SnapPy/16149_T464.tri')
D=snappy.Manifold('/home/cris/SnapPy/16149_T537.tri')
coversA=A.covers(6,method='gap')
coversB=B.covers(6,method='gap')
coversC=C.covers(6,method='gap')
coversD=D.covers(6,method='gap')
[X.homology() for X in coversA]
[X.homology() for X in coversB]
[X.homology() for X in coversC]
[X.homology() for X in coversD]